    "version": "0.2.0",
    "configurations": [
        {
            "name": "Create Web App",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/deployment/CreateWithBicep.ps1 $true",
            "cwd": "${cwd}"
        },
        {
            "name": "Delete Web App",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/deployment/CreateWithBicep.ps1 $false",
            "cwd": "${cwd}"
        },
        {
            "name": "C#: BlazorApp Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/BlazorApp/BlazorApp.csproj"
        }

        

    ]
}